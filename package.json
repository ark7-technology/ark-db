{
  "name": "@ark7/model-mongoose",
  "version": "1.0.0",
  "description": "Type-script friendly, object-oriented, database modeling framework",
  "types": "index.d.ts",
  "main": "index.js",
  "scripts": {
    "compile:src": "tsc --build src/tsconfig.json",
    "compile:test": "npm run compile:src && tsc --build test/tsconfig.json",
    "test": "npm run compile:src && NODE_ENV=test node_modules/.bin/mocha --trace-warnings --require ts-node/register \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ark7-technology/model-mongoose.git"
  },
  "keywords": [
    "ark7",
    "typescript",
    "database",
    "mongoose",
    "mongodb"
  ],
  "author": "yz@ark7.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ark7-technology/model-mongoose/issues"
  },
  "homepage": "https://github.com/ark7-technology/model-mongoose#readme",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/should": "^13.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^8.1.3",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "ts-transformer-interface": "^1.6.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@ark7/model": "^1.0.3",
    "@types/mongoose": "^5.7.36",
    "@types/underscore": "^1.10.24",
    "mongoose": "^5.10.9",
    "reflect-metadata": "^0.1.13",
    "underscore": "^1.11.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "renke"
    }
  }
}
